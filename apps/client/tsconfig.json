{
  "extends": "tsconfig/nextjs.json",
  "compilerOptions": {
    "baseUrl": ".",
    "plugins": [{ "name": "next" }],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts",
  "pages/**/*", "**/*.ts", "src/**/*", "components/**/*", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
